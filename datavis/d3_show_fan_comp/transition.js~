var data2002 = [];
var data2014 = [];

for(var i = 0; i < bothData.length; i++){
    if(bothData[i]["feature"] === "Memes"){
        data2002.push(bothData[i]);
    }else if(bothData[i]["feature"] === "Hashtags"){
        data2014.push(bothData[i]);
    }
}

function change(value){
    if(value === 'Memes'){
        update(data2002);
    }else if(value === 'Hashtags'){
        update(data2014);
    }
}

function update(data){

    console.log(data);
    data = data.sort(function(a, b) {
	return d3.ascending(a.ratio, b.ratio);
    });
    console.log(data);

    
    xChart.domain(data.map(function(d) {
		      return d.show;
		  }));
    
    yChart.domain( [0, d3.max(data, function(d){ return + d.ratio; })] );
    var barWidth = width / data.length;

    
    var bars = chart.selectAll(".bar")
        .data(data, function(d){ return d.show; });
    bars.exit()
        .remove();
    bars.enter()
        .append("rect")
        .attr("class", "bar")
        .attr("width", xChart.rangeBand())
        .attr("x", function(d) { return xChart(d.show) + xChart.rangeBand()*0.5; })
    //	.style("fill", function(d) { return color(d.rate) })
    
    //  .attr("x", function(d, i){return i * barWidth+1 })
        .attr("y",function(d){ return yChart(d.ratio); })
        .attr("height",function(d){ return height - yChart(d.ratio); })
        .attr("width", barWidth -8)
        .attr('fill', function(d){
            if(d.feature === "Hashtags"){
                return'#ea5454'
            }else{
                return'#4e97c4'
            }
            
        });
    bars.transition()
        .duration(800)
        .ease('linear')//d3.easeLinear)
        .attr("x", function(d) { return xChart(d.show) + xChart.rangeBand()*0.5; })
        .attr('y', function(d){ return yChart(d.ratio); })
        .attr('height', function(d){ return height - yChart(d.ratio); })
        .style('fill', function(d){
    	    console.log(d.year);
            if(d.feature === "Hashtags"){
    		return '#ea5454'
            } else {
    		return '#4e97c4'
            }
        });
    
    chart.select('.y').call(yAxis);


    
    
    chart.select('.xAxis')
        .attr("transform", "translate(0," + height + ")")
        .call(xAxis)
        .selectAll("text")
        .style("text-anchor", "end")
        .attr("dx", "-.8em")
        .attr("dy", ".0em")
        .attr("transform", function(d){
            return "rotate(-90)";
        });
}

var margin = {top: 20, right: 50, bottom: 250, left: 50};
var width = 900;
var height = 400;

var chart = d3.select("#chart").append("svg")
    .attr("width", width + margin.left + margin.right)
    .attr("height", height + margin.top + margin.bottom)
    .append("g")
    .attr("transform", "translate(" + margin.left + "," + margin.top + ")");

//var xChart = d3.scaleBand()
//       .range([0, width])
//	.paddingOuter(0.25);
var xChart = d3.scale.ordinal()
    .rangeRoundBands([0, width], 0.05);


var yChart = d3.scale.linear()
    .range([height, 0]);

//    var xAxis = d3.axisBottom(xChart);
//    var yAxis = d3.axisLeft(yChart);


var xAxis = d3.svg.axis()
    .scale(xChart)
    .orient("bottom");

var yAxis = d3.svg.axis()
    .scale(yChart)
    .orient("left");



chart.append("g")
    .attr("class", "y axis")
    .call(yAxis)

chart.append("g")
    .attr("class", "xAxis")
//    .attr("transform", "translate(0," + height + ")")
//    .call(xAxis)
//    .selectAll("text")
//    .style("text-anchor", "end")
//    .attr("dx", "-.8em")
//    .attr("dy", ".0em")
//    .attr("transform", function(d){
//        return "rotate(-90)";
//    });
;
chart.append("text")
    .attr("transform", "translate(-35," +  (height+margin.bottom)/2 + ") rotate(-90)")
    .text("Ratio");

chart.append("text")
    .attr("transform", "translate(" + (width/2) + "," + (height + margin.bottom - 5) + ")")
    .text("Shows");

update(data2002);

